<h3>Add new event<h3>

<%= form_with model: @event, local: true do |f| %>
  <%= f.label "Title:" %><br>
  <%= f.text_field :title %><br>
  <%= f.label "Location:" %><br>
  <%= f.text_field :location %><br>
  <%= f.label "Date:" %><br>
  <%= f.date_field :date %><br><br>
  <%= f.submit "Submit" %>
<% end %>